<h1>this are all the article</h1>
<table>
  <thead>
    <th rowspan= "3">Title</th>
    <th rowspan= "3">Description</th>
    <th rowspan= "3">Show</th>
  </thead>
  <tbody>
    <% @articles.each do |article| %>
      <tr>
        <td><%= article.title %></td>
        <td><%= article.description %></td>
        <td><%= link_to "Show", article_path(article[:id]) %></td>
        <td><%= link_to "Edit", edit_article_path(article[:id]) %></td>
        <td><%= link_to "Delete", article_path(article[:id]), method: :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= button_to "New Post", { :action => "new" }, :method => :get %>
  