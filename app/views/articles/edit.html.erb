<h1>Edit Article</h1> 
<div>
  <%= raw display_error(@article) %>
  <%= raw list_errors(@article) %>
</div>
<div>
  
</div>

<%= form_with(model: @article, local: true) do |f| %>

  <p>
    <%= f.label :title %><br/>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :description %><br/>
    <%= f.text_area :description %>
  </p>

  <p>
    <%= f.submit %>
  </p>

<% end %>
